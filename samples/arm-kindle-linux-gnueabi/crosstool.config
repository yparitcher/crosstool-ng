CT_CONFIG_VERSION="3"
CT_OBSOLETE=y
CT_EXPERIMENTAL=y
CT_STRIP_TARGET_TOOLCHAIN_EXECUTABLES=y
CT_ARCH_ARM=y
CT_ARCH_TUNE="arm1136jf-s"
CT_ARCH_ARCH="armv6j"
CT_ARCH_FPU="vfp"
CT_ARCH_FLOAT_SOFTFP=y
CT_TARGET_CFLAGS="-mno-unaligned-access"
CT_TOOLCHAIN_PKGVERSION="NiLuJe"
CT_TARGET_VENDOR="kindle"
CT_KERNEL_LINUX=y
CT_LINUX_V_2_6_27=y
CT_BINUTILS_LINKER_LD_GOLD=y
CT_BINUTILS_GOLD_THREADS=y
CT_BINUTILS_LD_WRAPPER=y
CT_BINUTILS_PLUGINS=y
CT_LIBC_GLIBC=y
CT_GLIBC_V_2_9=y
CT_GLIBC_KERNEL_VERSION_CHOSEN=y
CT_GLIBC_MIN_KERNEL_VERSION="2.6.22"
CT_GCC_USE_LINARO=y
# CT_CC_GCC_STATIC_LIBSTDCXX is not set
CT_CC_GCC_SYSTEM_ZLIB=y
# CT_CC_GCC_USE_GRAPHITE is not set
# CT_CC_GCC_ENABLE_TARGET_OPTSPACE is not set
CT_CC_LANG_CXX=y
